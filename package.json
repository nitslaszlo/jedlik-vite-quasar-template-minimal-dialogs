{
  "name": "jedlik-vite-quasar-minimal",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --port 8080 --open",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --ignore-path .gitignore --fix",
    "format": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,scss,json,md}",
    "test": "vitest --run --coverage --reporter verbose",
    "test:coverage": "vitest run --coverage",
    "cy:open": "npx cypress open --browser=edge",
    "cy:run": "npx cypress run"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.5",
    "@vueuse/core": "^10.3.0",
    "@vueuse/math": "^10.3.0",
    "@vueuse/shared": "^10.3.0",
    "axios": "^1.4.0",
    "pinia": "^2.1.6",
    "pinia-plugin-persist": "^1.0.0",
    "quasar": "^2.12.5",
    "tslib": "^2.6.2",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4",
    "vue3-google-login": "^2.0.20"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.12.3",
    "@pinia/testing": "^0.1.3",
    "@quasar/vite-plugin": "^1.4.1",
    "@types/node": "^20.5.4",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-vue": "^4.3.3",
    "@vitest/coverage-istanbul": "^0.34.2",
    "@vue/test-utils": "^2.4.1",
    "cypress": "^12.17.4",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "happy-dom": "^10.11.0",
    "sass": "1.32.12",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.2",
    "vue-tsc": "^1.8.8"
  }
}
