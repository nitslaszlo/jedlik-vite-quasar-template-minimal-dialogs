{
  "name": "jedlik-vite-quasar-minimal",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --port 8080 --open",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --ignore-path .gitignore --fix",
    "format": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,scss,json,md}",
    "test": "vitest --run --coverage --reporter verbose",
    "test:coverage": "vitest run --coverage",
    "cy:open": "npx cypress open --browser=edge",
    "cy:run": "npx cypress run"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.3",
    "@vueuse/core": "^10.1.2",
    "@vueuse/math": "^10.1.2",
    "@vueuse/shared": "^10.1.2",
    "axios": "^1.4.0",
    "pinia": "^2.0.36",
    "pinia-plugin-persist": "^1.0.0",
    "quasar": "^2.12.0",
    "tslib": "^2.5.0",
    "vue": "^3.3.2",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.0",
    "vue3-google-login": "^2.0.15"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@pinia/testing": "^0.1.0",
    "@quasar/vite-plugin": "^1.3.3",
    "@types/node": "^20.1.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-istanbul": "^0.31.0",
    "@vue/test-utils": "^2.3.2",
    "cypress": "^12.12.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.12.0",
    "happy-dom": "^9.18.3",
    "sass": "1.32.12",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.5"
  }
}
